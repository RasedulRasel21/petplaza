<style>
  #shopify-section-announcement-bar{
    display: none;
  }

  .header-style-1{
    background-color:{{ section.settings.header_background_color }};
  }
  .header-style-1 select{
     background-color:{{ section.settings.header_background_color }};
     border-color:{{ section.settings.header_background_color }};
  }

  .header-style-1 .header-bottom .switcher label,
  .header-style-1 .header-bottom .switcher .currency-picker{
    color:{{ section.settings.header_text_color }};
  }

  .header-style-1 .header-top .header-top-nav .item > a,
  .header-style-1 .header-top .header-links li a{
    color:{{ section.settings.header_link_color }};
  }

  .header-style-1 .header-top .header-top-nav .item:hover > a,
  .header-style-1 .header-top .header-links li a:hover{
    color:{{ section.settings.header_link_hover_color }};
  }

  .header-style-1 .header-bottom .switcher:hover label,
  .header-style-1 .header-bottom .switcher:hover .currency-picker,
  .header-style-1 .header-bottom .switcher:hover .language-name{
      color:{{ section.settings.header_link_hover_color }};
  }

