<header class="header header-desktop header-style-1 {% if section.settings.menu != blank %} header--has-menu{% endif %}">
  <div class="header-top">
    <div class="container">
      <div class="hd-top-content clearfix">
        <ul class="header-links">
          <li>
            <a href="{{section.settings.header_link1_url }}">
              {% if section.settings.header_link1_icon != blank %}
              <img loading="lazy" src="{{ section.settings.header_link1_icon | image_url: width: 30 }}">
              {% else %}
              <img loading="lazy" src="//via.placeholder.com/14x14">
              {% endif %}
              {{ section.settings.header_link1_text }}
            </a>
          </li>
          <li>
            <a href="{{section.settings.header_link2_url }}">
              {% if section.settings.header_link2_icon != blank %}
              <img loading="lazy" src="{{ section.settings.header_link2_icon | image_url: width: 30 }}">
              {% else %}
              <img loading="lazy" src="//via.placeholder.com/18x14">
              {% endif %}
              {{ section.settings.header_link2_text }}
            </a>
          </li>
        </ul>

        <ul class="header-top-nav">
          {%- for link in section.settings.header_top_nav.links -%}
            <li class="item {% if link.links != blank %} has-dropdown {% endif %}">
              <a
                href="{{ link.url }}"
                class="{% if link.current %} active {% endif %}"
                {% if link.current %}
                  aria-current="page"
                {% endif %}
              >
                {{ link.title | escape }}
                {% if link.links != blank %}
                  <span class="icon-keyboard_arrow_down"></span>
                {% endif %}
              </a>

              {%- if link.links != blank -%}
                <ul class="header-top-nav-dropdown">
                  {%- for childlink in link.links -%}
                    <li class="{%- if childlink.links != blank -%} has-child {% endif %}">
                      {%- if childlink.links == blank -%}
                        <a
                          href="{{ childlink.url }}"
                          class="{% if childlink.current %} active {% endif %}"
                          {% if childlink.current %}
                            aria-current="page"
                          {% endif %}
                        >
                          {{ childlink.title | escape }}
                        </a>
                      {%- else -%}
                        <a
                          href="{{ childlink.url }}"
                          class="{% if childlink.current %} active {% endif %}"
                          {% if childlink.current %}
                            aria-current="page"
                          {% endif %}
                        >
                          {{ childlink.title | escape }}
                        </a>

                        <ul class="list-sub-items">
                          {%- for grandchildlink in childlink.links -%}
                            <li>
                              <a
                                href="{{ grandchildlink.url }}"
                                class="{% if grandchildlink.current %} active {% endif %}"
                                {% if grandchildlink.current %}
                                  aria-current="page"
                                {% endif %}
                              >
                                {{ grandchildlink.title | escape }}
                              </a>
                            </li>
                          {%- endfor -%}
                        </ul>
                      {%- endif -%}
                    </li>
                  {%- endfor -%}
                </ul>
              {%- endif -%}
            </li>
          {%- endfor -%}
          
        </ul>
      </div>
    </div>
  </div>

  <div class="header-bottom">
    <div class="container">
      <div class="menu-container clearfix">
        <div class="logo-container">
          <h2 class="logo-content">
            <strong class="shop-name">{{ shop.name }}</strong>

            <a href="{{ routes.root_url }}">
              {% if section.settings.logo != blank %}
              <img loading="lazy" src="{{ section.settings.logo | image_url: width: 184 }}" alt="{{ shop.name }}">
              {% else %}
              <img loading="lazy" src="//via.placeholder.com/184x44">
              {% endif %}
              
            </a>
          </h2>
        </div>
        <div class="header-megamenu">
          <div class="megamenu-horizontal" data-element-mobile="menu-mobile">
            <div class="horizontal-content" data-mega-desktop="horizontal" data-mobile-content="content-mobile" class="content-mobile">
              <ul class="menu-items">
                {% for block in section.blocks %}
                  {% if block.type == 'horizontal' %}
                    {% if block.settings.category_name != blank %}
                      {% render 'bs-megamenu-item', block: block %}
                    {% endif %}
                  {% endif %}
                {% endfor %}
              </ul>
            </div>
          </div>
        </div>
        <div id="mobile-megamenu"></div>
        

        <div class="customer-wrapper">
          <div class="r-content">
            {% render 'language-theme' %}

            {% render 'currency-picker' %}
            
            <span class="ly-custom-1000"></span>

            <div class="search-header">
              <a class="btn-search-popup" href="#">
                <span>{{ 'general.search.search' | t }}</span>
              </a>

              <div class="overlay-search" data-action="close-search"></div>

              <div class="search-popup-container">
                <a class="btn-close" href="#" data-action="close-search"><span>Close</span></a>

                <div class="container">
                  <div class="search-content" id="search-desktop" data-element-mobile="search-mobile">
                    <form action="{{ routes.search_url }}" method="get" role="search" class="search search-modal__form">
                      <div class="field">
                        <input
                          class="search__input"
                          id="Search-In-Modal"
                          type="search"
                          name="q"
                          value="{{ search.terms | escape }}"
                          placeholder="{{ 'general.search.placeholder' | t }}"
                        >
                        <input type="hidden" name="shop_url" value={{ shop.url }}>
                        <input type="hidden" name="options[prefix]" value="last">
                        <button class="search__button field__button" aria-label="{{ 'general.search.search' | t }}">
                          <svg class="icon icon-search" aria-hidden="true" focusable="false" role="presentation">
                            <use href="#icon-search"/>
                          </svg>
                        </button>
                      </div>

                      {% if section.settings.search_navigation.links.size %}
                        <div class="top-search-collection">
                          <span>{{ 'general.search.top_collection' | t }}</span>
                          <ul class="menu">
                            {% for link in section.settings.search_navigation.links %}
                              <li class="menu-link">
                                <a href="{{ link.url }}">{{ link.title }}</a>
                              </li>
                            {% endfor %}
                          </ul>
                        </div>
                      {% endif %}
                    </form>

                    <div id="search-popup-results"></div>
                  </div>
                </div>
              </div>
            </div>

            {%- if shop.customer_accounts_enabled -%}
              <div class="header-links">
                <div class="dropdown-container">
                  <ul class="header-links-inner">
                    <li>
                      <a
                        href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}"
                        class="header__icon header__icon--account focus-inset{% if section.settings.menu != blank %} small-hide{% endif %}"
                      >
                        <span>
                          {%- liquid
                            if customer
                              echo 'customer.account_fallback' | t
                            else
                              echo 'customer.log_in' | t
                            endif
                          -%}
                        </span>
                      </a>
                    </li>
                  </ul>
                </div>
              </div>
            {%- endif -%}

            <div class="header-wishlist">
              <a href="/pages/wishlist" title="{{ 'layout.customer.wishlist' | t }}"
                ><span>{{ 'layout.customer.wishlist' | t }}</span></a
              >
            </div>

            {%- render 'minicart' -%}
          </div>
        </div>
      </div>
    </div>
  </div>
</header>
<div id="bzo-header-mobile" class="header-mobile" data-breakpoint="1024">
    <div class="mobile-top">
      <div class="mobile-top-content clearfix">
        <div id="mobile-nav" class="mobile-nav">
